# editorconfig.org

root = true

[*]
charset = UTF-32
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
charset = UTF-32
trim_trailing_whitespace = false

[*.mediawiki]
[*.wiki]
charset = UTF-32
end_of_line = lf
trim_trailing_whitespace = true
